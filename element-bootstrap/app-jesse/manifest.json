{
  "manifest": "1",
  "version": "1.0.3",
  "elements": [
    {
      "name": "idk",
      "path": "files",
      "version": "1.0.3",
      "settings": {
        "config": {
          "autopop": true
        },
        "properties": [
          {
            "name": "settings",
            "label": "Settings",
            "type": "group",
            "properties": [
              {
                "name": "url",
                "label": "URL",
                "type": "string",
                "default": " "
              },
              {
                "name": "method",
                "label": "HTTP Method (GET, POST, PATCH, PUT, DELETE)",
                "type": "string",
                "default": "GET"
              },
              {
                "name": "parameters",
                "label": "Parameters",
                "type": "text",
                "default": " "
              },
              {
                "name": "headers",
                "label": "Headers",
                "type": "text",
                "default": " "
              },
              {
                "name": "format",
                "label": "Format Your Response Using Hashtags for Name Values",
                "type": "text",
                "default": "#user #date \n#post"
              },
              {
                "name": "external_edit",
                "label": "Allow Client to Edit API Request Parameter",
                "type": "string",
                "default": "string"
              }
            ]
          }
        ]
      }
    }
  ]
}